{
	"name": "gatsby-starter-advanced",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby.",
	"version": "0.1.0",
	"author": "Niels van Galen Last <nvangalenlast@gmail.com>",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.21",
		"@fortawesome/free-brands-svg-icons": "^5.10.1",
		"@fortawesome/free-regular-svg-icons": "^5.10.1",
		"@fortawesome/free-solid-svg-icons": "^5.10.1",
		"@fortawesome/react-fontawesome": "^0.1.4",
		"gatsby": "^2.13.67",
		"gatsby-plugin-manifest": "^2.2.5",
		"gatsby-plugin-offline": "^2.2.6",
		"gatsby-plugin-react-helmet": "^3.1.3",
		"gatsby-plugin-sass": "^2.1.8",
		"node-sass": "^4.12.0",
		"prop-types": "^15.7.2",
		"react": "^16.9.0",
		"react-dom": "^16.9.0",
		"react-helmet": "^5.2.1",
		"react-images": "^1.0.0"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.2",
		"eslint": "^5.12.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-loader": "^2.2.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^1.7.0",
		"eslint-watch": "^6.0.0",
		"gatsby-plugin-eslint": "^2.0.5",
		"prettier": "^1.18.2",
		"prettier-eslint": "^9.0.0",
		"webpack": "^4.39.2"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"main": "n/a",
	"scripts": {
		"build": "gatsby build",
		"dev": "rm -rf .cache && yarn develop",
		"develop": "gatsby develop",
		"serve": "gatsby serve",
		"start": "yarn dev",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
		"build:pp": "gatsby build --prefix-paths",
		"build:gh": "yarn clean && yarn build:pp && gh-pages -d public",
		"clean": "rm -rf public && rm -rf .cache",
		"lint": "yarn lint:js && yarn lint:md",
		"lint:js": "esw --ext .js,.jsx --color .",
		"lint:js:watch": "yarn lint:js --watch",
		"lint:md": "remark content/posts/",
		"write-good": "write-good $(glob 'content/posts/**/*.md')",
		"format:js": "prettier '**/*.{js,jsx}' --write"
	},
	"remarkConfig": {
		"plugins": [
			"remark-preset-lint-recommended"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nielsgl/gatsby-starter-kit"
	},
	"bugs": {
		"url": "https://github.com/nielsgl/gatsby-starter-kit/issues"
	}
}
