{
	"name": "gatsby-starter-advanced",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby.",
	"version": "0.1.0",
	"author": "Niels van Galen Last <nvangalenlast@gmail.com>",
	"dependencies": {
		"gatsby": "^2.13.65",
		"gatsby-image": "^2.2.8",
		"gatsby-plugin-catch-links": "^2.1.3",
		"gatsby-plugin-feed": "^2.3.6",
		"gatsby-plugin-google-analytics": "^2.1.7",
		"gatsby-plugin-lodash": "^3.1.2",
		"gatsby-plugin-manifest": "^2.2.5",
		"gatsby-plugin-nprogress": "^2.1.2",
		"gatsby-plugin-offline": "^2.2.6",
		"gatsby-plugin-react-helmet": "^3.1.3",
		"gatsby-plugin-sharp": "^2.2.11",
		"gatsby-plugin-sitemap": "^2.2.6",
		"gatsby-plugin-twitter": "^2.1.2",
		"gatsby-remark-autolink-headers": "^2.1.3",
		"gatsby-remark-copy-linked-files": "^2.1.6",
		"gatsby-remark-images": "^3.1.12",
		"gatsby-remark-prismjs": "^3.3.5",
		"gatsby-remark-responsive-iframe": "^2.2.4",
		"gatsby-source-filesystem": "^2.1.9",
		"gatsby-transformer-remark": "^2.6.14",
		"gatsby-transformer-sharp": "^2.2.6",
		"lodash": "^4.17.15",
		"moment": "^2.24.0",
		"prismjs": "^1.17.1",
		"react": "^16.9.0",
		"react-disqus-comments": "^1.4.0",
		"react-dom": "^16.9.0",
		"react-helmet": "^5.2.1",
		"react-share": "^3.0.0",
		"react-twitter-widgets": "^1.7.1",
		"url-join": "^4.0.1"
	},
	"devDependencies": {
		"cli-glob": "^0.1.0",
		"eslint": "^6.1.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"eslint-watch": "^6.0.0",
		"gh-pages": "^2.1.1",
		"prettier": "^1.18.2",
		"remark-cli": "^7.0.0",
		"remark-preset-lint-recommended": "^3.0.3",
		"stylefmt": "^6.0.3",
		"stylelint": "^10.1.0",
		"stylelint-config-standard": "^18.3.0",
		"write-good": "^1.0.2"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"main": "n/a",
	"scripts": {
		"build": "gatsby build",
		"dev": "yarn develop",
		"develop": "gatsby develop",
		"serve": "gatsby serve",
		"start": "yarn dev",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
		"build:pp": "gatsby build --prefix-paths",
		"build:gh": "yarn clean && yarn build:pp && gh-pages -d public",
		"clean": "rm -rf public && rm -rf .cache",
		"lint": "yarn lint:js && yarn lint:md",
		"lint:js": "esw --ext .js,.jsx --color .",
		"lint:js:watch": "yarn lint:js --watch",
		"lint:md": "remark content/posts/",
		"write-good": "write-good $(glob 'content/posts/**/*.md')",
		"format:js": "prettier '**/*.{js,jsx}' --write"
	},
	"remarkConfig": {
		"plugins": [
			"remark-preset-lint-recommended"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nielsgl/gatsby-starter-kit"
	},
	"bugs": {
		"url": "https://github.com/nielsgl/gatsby-starter-kit/issues"
	}
}
