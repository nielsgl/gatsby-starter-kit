{
	"name": "gatsby-starter-advanced",
	"private": true,
	"description": "A simplified bare-bones starter for Gatsby.",
	"version": "0.1.0",
	"author": "Niels van Galen Last <nvangalenlast@gmail.com>",
	"dependencies": {
		"@gatsby-contrib/gatsby-transformer-ipynb": "^0.5.8",
		"gatsby": "^2.13.67",
		"gatsby-plugin-catch-links": "^2.0.13",
		"gatsby-plugin-feed": "^2.2.0",
		"gatsby-plugin-google-analytics": "^2.0.19",
		"gatsby-plugin-lodash": "^3.0.5",
		"gatsby-plugin-manifest": "^2.2.5",
		"gatsby-plugin-nprogress": "^2.0.10",
		"gatsby-plugin-offline": "^2.2.6",
		"gatsby-plugin-react-helmet": "^3.1.3",
		"gatsby-plugin-sass": "^2.1.8",
		"gatsby-plugin-sharp": "^2.0.37",
		"gatsby-plugin-sitemap": "^2.1.0",
		"gatsby-plugin-twitter": "^2.0.13",
		"gatsby-remark-autolink-headers": "^2.0.16",
		"gatsby-remark-copy-linked-files": "^2.0.12",
		"gatsby-remark-images": "^3.0.11",
		"gatsby-remark-prismjs": "^3.2.9",
		"gatsby-remark-responsive-iframe": "^2.1.1",
		"gatsby-source-filesystem": "^2.0.33",
		"gatsby-transformer-remark": "^2.3.12",
		"gatsby-transformer-sharp": "^2.1.19",
		"lodash": "^4.17.11",
		"moment": "^2.24.0",
		"node-sass": "^4.12.0",
		"prismjs": "^1.16.0",
		"prop-types": "^15.7.2",
		"react": "^16.9.0",
		"react-disqus-comments": "^1.4.0",
		"react-dom": "^16.9.0",
		"react-helmet": "^5.2.1",
		"react-images": "^1.0.0",
		"react-share": "^2.4.0",
		"react-twitter-widgets": "^1.7.1",
		"url-join": "^4.0.0"
	},
	"devDependencies": {
		"babel-eslint": "^10.0.2",
		"cli-glob": "^0.1.0",
		"eslint": "^5.12.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-loader": "^2.2.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^1.7.0",
		"eslint-watch": "^6.0.0",
		"gatsby-plugin-eslint": "^2.0.5",
		"gh-pages": "^2.0.1",
		"prettier": "^1.18.2",
		"prettier-eslint": "^9.0.0",
		"remark-cli": "^6.0.1",
		"remark-preset-lint-recommended": "^3.0.2",
		"stylefmt": "^6.0.3",
		"stylelint": "^10.0.1",
		"stylelint-config-standard": "^18.3.0",
		"webpack": "^4.39.2",
		"write-good": "^1.0.1"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"main": "n/a",
	"scripts": {
		"build": "gatsby build",
		"dev": "yarn clean && yarn develop",
		"develop": "gatsby develop",
		"serve": "gatsby serve",
		"start": "yarn dev",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
		"build:pp": "gatsby build --prefix-paths",
		"build:gh": "yarn clean && yarn build:pp && gh-pages -d public",
		"clean": "rm -rf public && rm -rf .cache",
		"lint": "yarn lint:js && yarn lint:md",
		"lint:js": "esw --ext .js,.jsx --color .",
		"lint:js:watch": "yarn lint:js --watch",
		"lint:md": "remark content/posts/",
		"write-good": "write-good $(glob 'content/posts/**/*.md')",
		"format:js": "prettier '**/*.{js,jsx}' --write"
	},
	"remarkConfig": {
		"plugins": [
			"remark-preset-lint-recommended"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nielsgl/gatsby-starter-kit"
	},
	"bugs": {
		"url": "https://github.com/nielsgl/gatsby-starter-kit/issues"
	}
}
